<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    
    <!-- Spring Boot DevTools - Development Only -->
    <suppress>
        <notes>Spring Boot DevTools is only used in development and not included in production builds</notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.boot/spring-boot-devtools@.*$</packageUrl>
    </suppress>
    
    <!-- H2 Database - In-memory usage only -->
    <suppress>
        <notes>H2 Database is used in-memory mode only, not exposed to external connections</notes>
        <packageUrl regex="true">^pkg:maven/com\.h2database/h2@.*$</packageUrl>
        <cve>CVE-2018-10054</cve>
        <cve>CVE-2018-14335</cve>
    </suppress>
    
    <!-- Tomcat Embed - Embedded usage only -->
    <suppress>
        <notes>Tomcat is used in embedded mode within Spring Boot, not as standalone server</notes>
        <packageUrl regex="true">^pkg:maven/org\.apache\.tomcat\.embed/.*@.*$</packageUrl>
    </suppress>
    
</suppressions>